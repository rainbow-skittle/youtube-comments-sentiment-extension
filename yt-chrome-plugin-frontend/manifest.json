  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://ec2-18-215-169-8.compute-1.amazonaws.com:5001/*",
    "https://www.googleapis.com/*"
  ]
}